/* 轮播图 */
html,
body {
	position: relative;
	height: 100%;
}

body {
	font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
	color: #000;
	margin: 0;
	padding: 0;
}

.swiper-container {
	width: 100%;
	height: 100%;
}

.swiper-slide {
	text-align: center;
	font-size: 18rpx;
	background: #fff;

	/* Center slide text vertically */
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
}

.swiper-box .swiper-slide,
.swiper-change .swiper-slide {
	height: 222.82rpx;
	padding-bottom: 18.07rpx;
	width: 162.97rpx !important;
	display: block;
	margin-right: 18.07rpx;
}

.swiper-box .swiper-slide image .swiper-change .swiper-slide image {
	width: 144.9rpx;
	height: 157.57rpx;
}

.swiper-box .swiper-slide .swiper-price,
.swiper-change .swiper-slide .swiper-price {
	color: #FF9347;
}

.swiper-box .swiper-slide,
.swiper-change .swiper-slide {
	font-size: 21.67rpx;
}

.swiper-box .swiper-slide .title,
.swiper-change .swiper-slide .title {
	height: 28.95rpx;
	line-height: 28.95rpx;
}

.swiper-box,
.swiper-change {
	display: flex;
	margin: 0 28.95rpx;
}

.swiper-box .swiper-scrollbar {
	/* opacity: 0; */
}

.swiper-box .swiper-scrollbar-drag,
.swiper-change .swiper-scrollbar-drag {
	width: 348.45rpx !important;
	background: rgba(0, 0, 0, .3);
	height: 7.5rpx !important;
}

.on {
	color: #FF9347;
}

.tab-swiper {
	padding: 18.0rpx 0;
	display: flex;
	justify-content: center;
	font-size: 25.35rpx;

}

.tab-swiper text {
	display: inline-block;
	width: 280.5rpx;
	text-align: center;
	height: 36.22rpx;
}

.left-title {
	position: relative;
}

.left-title::after {
	content: "";
	width: 1rpx;
	height: 36.225rpx;
	background: #DBDBDB;
	display: inline-block;
	position: absolute;
	right: 0;
}

.title-wrap {
	display: flex;
	align-items: center;
}

.swiper-change {
	display: none;
}

.slide-image {
	width: 80rpx;
	height: 87rpx;
}
