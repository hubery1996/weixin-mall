.box {
	padding: 0 3.86vw;
	background: white;
  font-size: 24rpx;
  display: flex;
  align-items: center;
}

.headbar {
	display: flex;
	justify-content: center;
	align-items: center;
	color: black;
	font-size: 4.10vw;
	height: 10.62vw;
	position: relative;
}


.headbar i {
	font-size: 4.10vw;
	width: 6vw;
	height: 100%;
	display: inline-block;
	line-height: 10.62vw;

}

.headbar-wrap navigator {
	position: absolute;
	left: 0;
	display: inline-block;
}

.top-title {
	background: white;
  height: 6vh;
}

.top-title .ul {
  height: 6vh;
	display: flex;
	justify-content: space-between;
}

.top-title .ul .li {
	flex: 1;
	display: flex;
	justify-content: center;
	height: 100%;
	align-items: center;
	position: relative;
  font-size: 24rpx;
}


.goods-wrap {
	margin: 2.32vw 2.07vw 0 2.07vw;
	background: white;
	border-radius: 2vw;
}

.goods-wrap .head {
	padding-left: 4.14vw;
	display: flex;
	justify-content: space-between;
	align-items: center;
	height: 9.17vw;
	font-size: 3.10vw;
	color: #71797F;
}

.goods-wrap .head .text {
	font-size: 3.38vw;
	color: black;
	margin-right: 3.54vw;
}

.goods-wrap .content {
	display: flex;
	justify-content: space-between;
	align-items: center;
	width: 87.43vw;
	height: 17.59vw;
	padding: 2.07vw 4.13vw;
	background: #F7F9FA;
}

.goods-wrap .info {
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.goods-wrap .info .title{
  font-size: 24rpx;
  display: -webkit-box;
   word-break: break-all;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  width: 70vw;
}
.goods-wrap .info image {
	margin-right: 2.07vw;
	width: 15.45vw;
	height: 17.59vw;
}

.goods-wrap .bottom {
	padding: 2.07vw 0;
	display: flex;
	justify-content: space-between;
	font-size: 3.10vw;
	color: #FF734C;
}

.goods-wrap .bottom .pay {
  justify-content: center;
  display: flex;
  align-items: center;
	width: 20.50vw;
	height: 7.24vw;
	border: 1rpx solid #FF734C;
  box-sizing: border-box;
	margin-right: 4.13vw;
}

.goods-wrap .bottom .pay navigator {
	width: 100%;
	height: 100%;
	display: inline-block;
	text-align: center;
	line-height: 7.24vw;
}

.empty .image {
	width: 31.64vw;
	height: 36.71vw;
	margin-bottom: 4.39vw;
  margin: 0 auto;
	background: url(data:image/png;base64,);
}

.empty {
	text-align: center;
	padding: 7.75vw 0;
	background: white;
}

.empty navigator {
	margin: 4.39vw 0;
	display: inline-block;
	width: 26.32vw;
	height: 9.31vw;
	line-height: 9.31vw;
	background: #FF734C;
	color: white;
	border-radius: 5vw;
}

swiper{
	height: 94vh;
}
swiper-item {
	height: 90vh;
	overflow: auto;
	background: #E9ECF0;
}

.top-title .active::after{
	content: '';
	width: 50%;
	height: 6rpx;
	background: #FF734C;
	position: absolute;
	bottom: 0;
	left: 0;
	margin-left: 25%;
	border-top-left-radius: 40rpx;
	border-top-right-radius: 40rpx;
}
