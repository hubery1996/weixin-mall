	content: "";
	display: block;
	position: absolute;
	top: 3rpx;
	left: 0;
	width: 0;
	height: 0;
	font-size: 0;
	border: 7.5rpx solid transparent;
	border-bottom-color: #B4BABF;
}

.filter-item-price:after {
	content: "";
	display: block;
	position: absolute;
	top: 20rpx;
	left: 0;
	width: 0;
	height: 0;
	font-size: 0;
	border: 7.5rpx solid transparent;
	border-top-color: #B4BABF;
}

